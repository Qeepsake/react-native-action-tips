{
  "name": "react-native-action-tips",
  "version": "2.0.1",
  "description": "Cross platform tooltip for React Native.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lukebrandonfarrell/react-native-action-tips.git"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "pre-commit": "lint-staged",
    "postinstall": "husky install"
  },
  "keywords": [
    "tips",
    "react-native",
    "tooltips",
    "tooltip",
    "popup",
    "javascript"
  ],
  "author": "Luke Brandon Farrell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukebrandonfarrell/react-native-action-tips/issues"
  },
  "homepage": "https://github.com/lukebrandonfarrell/react-native-action-tips#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "pinst": "^2.1.6",
    "prettier": "^1.19.1",
    "semantic-release": "^19.0.3",
    "typescript": "^3.9.9"
  },
  "hooks": {
    "pre-commit": "tsc && lint-staged"
  },
  "lint-staged": {
    "./src/**/*.+(js|jsx|ts|tsx)": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/react": "^16.9.9",
    "@types/react-native": "^0.60.21"
  }
}
